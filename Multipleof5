package Acharya.classes;

public class MultipleOfFive {

    
    public static void printMutliples(int arr[]) {
        int count = 0;

        for (int num : arr) {
            if (num % 5 == 0) {  
                count++;
            }
        }

        System.out.println("Number of elements divisible by 5: " + count);
    }

    public static void main(String[] args) {
        int[] arr = {5, 12, 15, 8, 20, 7};  

        printMutliples(arr); 
    }
}
