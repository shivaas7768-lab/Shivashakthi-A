package Acharya.classes;

public class PolymorphismDemo2 {

	public static void main(String[] args) {
		PolymorphismDemo2 polydemo = new PolymorphismDemo2();
		System.out.println("Square area:" + polydemo.area(2));
		System.out.println("Rectangle area:" + polydemo.area(10, 12));
		System.out.println("Circle area:" + polydemo.area(2.0));
	}

	public int area(int side) {
		int area = side * side;
		return area;
	}

	public int area(int length,int breadth) {
		int area = length*breadth;
		return area;

}
	public double area(double radius){
	double area = 3.14*radius*radius;
	return area;
}
}
