package Acharya.classes;
public class MultiplesCheck {

    public static void printMutliples(int arr[]) {
        int count = 0;

        for (int num : arr) {
            if (num % 3 == 0) {
                count++;
            }
        }

        System.out.println("Number of elements divisible by 3: " + count);
    }

    public static void main(String[] args) {
        int[] arr = {3, 8, 12, 14, 18, 7};  

        printMutliples(arr);  
    }
}
